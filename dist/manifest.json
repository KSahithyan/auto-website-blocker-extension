{
    "manifest_version": 2,
    "name": "Auto Website Blocker",
    "description": "A website extension, whcih lets you block websites automatically, based on time.",
    "version": "1.0",
    "background": {
        "scripts": ["src/background.js"]
    },
    "permissions": [
        "menus",
        "webRequest",
        "<all_urls>",
        "webRequestBlocking",
        "storage",
        "notifications"
    ]
}